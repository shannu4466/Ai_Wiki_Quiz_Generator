{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "strict": false,
    "allowJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}